
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="div_section">

    <div class="col-xs-12 no-padding">
        <div class="box box-solid">
            <div class="box-header with-border">
                <div class="col-xs-6 left no-padding"><h3 class="box-title" style="margin-top:7px;">Departman İşlemleri</h3></div>
                <div class="col-xs-6 text-right no-padding" id="box-header-buttons"></div>
            </div>
            <!-- /.box-header -->
            <div class="box-body">


                @Html.Partial("~/Views/Department/Partial/p_departmentForm.cshtml")

                <table id="tblDepartmentList" class="table-bordered table table-striped table-hover" cellspacing="0" data-page-length='5' style="width:100%;">
                    <thead>
                        <tr>
                            <th>Durum</th>
                            <th>Departman Adı</th>
                            <th>Id</th>
                        </tr>
                    </thead>
                </table>


            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>


</div>



